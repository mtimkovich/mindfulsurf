{
  "name": "Mindfulsurf",
  "version": "0.1.0",
  "homepage_url": "https://github.com/mtimkovich/mindfulsurf",
  "manifest_version": 3,
  "minimum_chrome_version": "95",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["inject.js"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "content_security_policy": {
    "extension_pages": "default-src 'self' 'wasm-unsafe-eval';"
  }
}
